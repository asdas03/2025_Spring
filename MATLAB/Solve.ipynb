   "source": [
    "from sympy import Matrix, symbols, solve\n",
    "\n",
    "x, y, z = symbols('x y z')\n",
    "\n",
    "A = Matrix([\n",
    "    [1,4,-1],\n",
    "    [4,1,2],\n",
    "    [3,-3,3]\n",
    "])\n",
    "\n",
    "B = Matrix([\n",
    "    [3],\n",
    "    [-1],\n",
    "    [-2]\n",
    "])\n",
    "\n",
    "eqs = A * Matrix([x, y, z]) - B\n",
    "\n",
    "solution = solve(eqs, (x, y, z))\n",
    "print(solution)\n"
   ]
  }
